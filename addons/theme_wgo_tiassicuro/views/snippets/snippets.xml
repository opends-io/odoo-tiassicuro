<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="theme_wgo_tiassicuro_snippets" inherit_id="website.snippets"
        name="Theme WGO TiAssicuro Snippets"
    >
        <xpath expr="//snippets[@id='snippet_structure']" position="before">
            <snippets id="theme_wgo_tiassicuro_snippet_structure" string="TiAssicuro">
                <t t-snippet="theme_wgo_tiassicuro.intro_snippet" string="Intro TiAssicuro"
                    group="intro">
                    <keywords>intro, tiassicuro</keywords>
                </t>
            </snippets>
        </xpath>
    </template>
</odoo>